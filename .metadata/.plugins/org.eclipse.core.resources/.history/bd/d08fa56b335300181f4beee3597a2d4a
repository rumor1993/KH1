package net.board.db;

public class Board {

	private int BOARD_NUM; 			// 글 번호
	private String BORAD_NAME; 		// 글 이름
	private String BORAD_PASS; 		// 글 비밀번호
	private String BORAD_SUBJECT;	// 글 제목 
	private String BORAD_CONTENT;	// 글 내용
	private String BORAD_FILE;		// 첨부될 파일명
	private int BORAD_RE_REF;		// 답변 글 작성시 참조되는 글의 번호
	private int BORAD_RE_LEV;		// 답변 글의 깊이
	private int BORAD_RE_SEQ;		// 답변 글의 순서
	private int BORAD_READCONUT;	// 글의 조회수
	private Date BORAD_DATE;			// 글의 작성 날짜
}
