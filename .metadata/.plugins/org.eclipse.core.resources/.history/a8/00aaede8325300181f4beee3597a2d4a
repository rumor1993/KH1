package net.board.db;

public class Board {

	BOARD_NUM NUMBER, -- 글번호
	BORAD_NAME VARCHAR2(30), -- 작성자
	BORAD_PASS VARCHAR2(30), -- 비밀번호
	BORAD_SUBJECT VARCHAR2(300), -- 제목
	BORAD_CONTENT VARCHAR2(4000), -- 내용
	BORAD_FILE VARCHAR2(50), -- 첨부될 파일명
	BORAD_RE_REF NUMBER, -- 답변 글 작성시 참조되는 글의 번호
	BORAD_RE_LEV NUMBER, -- 답변 글의 깊이
	BORAD_RE_SEQ NUMBER, -- 답변 글의 순서
	BORAD_READCONUT NUMBER, -- 글의 조회수
	BORAD_DATE DATE, -- 글의 작성 날자
}
